.phony: clean main test
all: main

test: test.o square.o in_out.o
	gcc square.o test.o in_out.o -o TEST

main: main.o square.o in_out.o
	gcc square.o main.o in_out.o -o MAIN

main.o: main.c
	gcc main.c -c -o main.o	
square.o: square.c
	gcc square.c -c -o square.o
in_out.o: in_out.c
	gcc in_out.c -c -o in_out.o
test.o: test.c
	gcc test.c -c -o test.o

clean:
	rm -rf *.o MAIN TEST